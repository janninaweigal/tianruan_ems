.editContainer{
  width:100%;
  box-sizing:border-box;
  padding:10rpx 30rpx;
}
.editContainer view{
  display:flex;
  flex-direction:row;
  justify-content:space-between;
  align-items:center;
  padding:20rpx 15rpx;
  box-sizing:border-box;
  border-bottom:1px solid #ccc;
}
.editContainer view input{
  text-align:right;
}
.text{
color: #aaa;
}