#home .tip-container {
  padding: 0px 10px 0px 10px;
  display: flex;
  flex-direction: row;
  justify-content:space-between;
}

#home .tip-container .tip-wrapper {
  display:flex;
  flex-direction:column;
  padding: 5rpx;
  margin-bottom: 5rpx;
}

#home .tip-container .order-wrapper {
  display:flex;
  flex-direction:row;
  justify-content:flex-start;
  align-items:center;
}
#home .tip-container .order-wrapper view{
  display:flex;
  flex-direction:column;
  justify-content:space-between;
  align-items:center;
  padding: 0rpx 10rpx;   
}

#home .swiper-container {
  width: 100%;
  height: 200px;
}

#home .swiper-container .slide-img {
  width: 100%;
  height: 200px;
  border-radius: 15rpx;
}

#home .module-container {
  width: 100%;
  display: flex;
  flex-wrap:wrap;
  box-sizing: border-box;
  padding:0rpx 20rpx;
  flex-direction:row;
  justify-content:space-between;
  margin-top: 60rpx;
}

 #home .module-container .box-wrapper{
  height: 120rpx;
  width: 120rpx;
  border:1px solid #ccc;
} 

/* 服务 */
.servicebox{
  display:flex;
  flex-direction:column;
  justify-content:center;
  align-items:center;
}
.servicebox .box-img{
  width:80rpx;
  height:80rpx;
}